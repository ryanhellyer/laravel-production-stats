{
    "name": "ryanhellyer/laravel-production-stats",
    "description": "Production-ready performance monitoring for Laravel - automatically track and inject performance statistics into responses",
    "keywords": ["laravel", "performance", "monitoring", "stats", "production", "load-time"],
    "homepage": "https://github.com/ryanhellyer/laravel-production-stats",
    "type": "library",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Ryan Hellyer",
            "email": "ryan@hellyer.kiwi"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "RyanHellyer\\ProductionStats\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RyanHellyer\\ProductionStats\\ProductionStatsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit"
    }
}
